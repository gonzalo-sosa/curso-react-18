{
  "version": 2,
  "builds": [
    {
      "src": "proyectos/game-hub/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "bun game-hub:build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/proyectos/game-hub/$1"
    }
  ],
  "installCommand": "bun game-hub:install"
}
